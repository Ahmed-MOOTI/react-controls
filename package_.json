    "react-markdown": "^2.4.5",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-connect": "^5.0.0",
    "redux-logic": "^0.11.7",
    "redux-mock-provider": "^1.0.0",
    "reselect": "^2.5.4",
    "rxjs": "^5.2.0",
    "select2": "^4.0.3",
