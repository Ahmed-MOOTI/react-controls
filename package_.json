    "@types/jest": "^18.1.1",
    "@types/react": "^15.0.21",
    "@types/react-dom": "^0.14.23",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.3.3",
    "babel-preset-es2015": "^6.18.0",
