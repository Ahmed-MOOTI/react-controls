{
  "name": "@up-group/up-react-controls",
  "version": "0.0.13",
  "description": "",
  "main": "./up-react-controls.js",
  "types": "./up-react-controls.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Up-Group/react-controls.git"
  },
  "keywords": [
    "UI",
    "React",
    "Health",
    "UpGroup"
  ],
  "author": "@UpGroup",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Up-Group/react-controls/issues"
  },
  "homepage": "https://github.com/Up-Group/react-controls#readme",
  "engines": {
    "node": "6.9.5",
    "npm": "3.10.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/testing/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/config/testing/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/",
      "/__mocks__/"
    ],
    "transform": {
      ".*": "<rootDir>/config/testing/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "scripts": {
    "precommit": "npm run lint --silent && npm run test",
    "build": "webpack --config ./webpack.config.js",
    "predeploy": "rimraf build/public/*.*",
    "deploy": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.prod.js",
    "postdeploy": "npm run compile",
    "compile": "./node_modules/.bin/tsc",
    "setup:yarn": "yarn",
    "preserve": "npm run compile",
    "lint": "tslint \"packages/**/*.ts{,x}\" && tslint \"src/**/*.ts{,x}\"",
    "postinstall": "npm run deploy",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "test:update": "npm run test -- --u",
    "publishProd": "npm version patch && npm publish",
    "publishBeta": "npm version patch && npm publish --tag beta"
  },
  "dependencies": {
    "@types/node": "^6.0.64",
    "@types/react": "^15.0.14",
    "@types/react-dom": "^0.14.23",
    "@types/sinon": "^1.16.35",
    "@types/webpack": "^2.2.11",
    "autoprefixer": "^6.7.6",
    "axios": "^0.15.3",
    "babel-polyfill": "^6.23.0",
    "cors": "^2.8.1",
    "draft-js": "^0.10.0",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "exports-loader": "^0.6.4",
    "express": "^4.15.0",
    "github-markdown-css": "^2.4.1",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^1.3.1",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "jquery": "^3.2.1",
    "markdown-loader": "^2.0.0",
    "mongoose": "^4.8.5",
    "morgan": "^1.8.1",
    "node-env-file": "^0.1.8",
    "openui": "1.0.1",
    "plop": "^1.7.4",
    "precss": "^1.4.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.2",
    "react-apollo": "^0.13.2",
