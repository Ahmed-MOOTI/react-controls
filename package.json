{
  "name": "@up-group/react-controls",
  "version": "0.0.379",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Up-Group/react-controls.git"
  },
  "keywords": [
    "UI",
    "React",
    "Health",
    "UpGroup"
  ],
  "author": "@UpGroup",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Up-Group/react-controls/issues"
  },
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "homepage": "https://github.com/Up-Group/react-controls#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/testing/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/config/testing/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/",
      "/__mocks__/"
    ],
    "transform": {
      ".*": "<rootDir>/config/testing/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf test/dist",
    "build": "tsc && copyfiles -u 1 src/Common/theming/icons/*.* dist/ && npm run copyCSS && npm run copyFont && npm run copyImg && npm run copySCSS",
    "copyCSS": "copyfiles -u 1 src/**/*.css dist/",
    "copySCSS": "copyfiles -u 1 src/**/*.scss dist/",
    "copyFont": "copyfiles -u 1 src/**/*.ttf dist/ && copyfiles -u 1 src/**/*.woff dist/ && copyfiles -u 1 src/**/*.eot dist/",
    "copyImg": " copyfiles -u 1 src/**/*.png dist/",
    "build-watch": "tsc -w",
    "prepublish": "npm run clean && npm run build ",
    "publishProd": "npm version patch && npm publish",
    "publishBeta": "npm version patch && npm publish --tag beta",
    "build-test": "npm run clean && webpack",
    "build-test-watch": "npm run clean && webpack --watch",
    "publishLocalToSchemaForm": "copyfiles dist/*.* dist/**/* package.json src/*.* src/**/* src/theming/icons/* ../react-schemaform/node_modules/@up-group/react-controls/",
    "publishLocalToApp": "copyfiles dist/*.* dist/**/* dist/**/*.* package.json src/*.* src/**/* src/**/*.* src/theming/icons/* ../react-app/node_modules/@up-group/react-controls/",
    "test": "jest --notify --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./docs"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.11",
    "@storybook/addon-info": "3.2.11",
    "@storybook/react": "3.2.11",
    "@types/chai": "^3.5.2",
    "@types/chai-enzyme": "^0.6.1",
    "@types/enzyme": "*",
    "@types/jest": "^21.1.5",
    "@types/jquery": "^2.0.41",
    "@types/moment": "^2.13.0",
    "@types/node": "^7.0.12",
    "@types/pure-render-decorator": "^0.2.27",
    "@types/react": "15.6.1",
    "@types/react-dom": "^0.14.23",
    "@types/react-select": "1.0.55",
    "@types/select2": "^4.0.34",
    "@types/webpack-env": "^1.13.0",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "file-loader": "0.11.1",
    "jest": "^21.1.5",
    "json-loader": "^0.5.4",
    "markdown-loader": "^2.0.0",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.5.1",
    "react-storybook-addon-info": "^3.5.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.16.1",
    "svg-inline-loader": "^0.7.1",
    "ts-loader": "2.0.3",
    "typescript": "2.3.4",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "yarn": "^0.21.3"
  },
  "dependencies": {
    "@blueprintjs/core": "1.27.0",
    "@blueprintjs/datetime": "1.22.0",
    "axios": "^0.16.1",
    "bootstrap": "^3.3.7",
    "ckeditor": "^4.7.3",
    "classnames": "^2.2.5",
    "color": "^1.0.3",
    "draft-js": "^0.10.0",
    "github-markdown-css": "^2.4.1",
    "inputmask-core": "^2.2.0",
    "interact.js": "^1.2.8",
    "jquery": "^3.2.1",
    "moment": "^2.19.1",
    "normalize.css": "^4.1.1",
    "object-assign": "^4.1.1",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-update": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-input-autosize": "1.1.0",
    "react-markdown": "^2.4.5",
    "react-maskedinput": "^3.3.4",
    "react-pure-render": "^1.0.2",
    "react-select": "1.0.0-rc.5",
    "react-sortable-hoc": "^0.6.1",
    "react-textarea-autosize": "^4.0.5",
    "react-tooltip": "^3.3.0",
    "select2": "^4.0.3",
    "styled-components": "^1.4.4",
    "typestyle": "^1.3.7",
    "validator": "^7.0.0"
  }
}
